    "watch:webpack": "webpack --mode=development --watch",
    "watch:tailwind": "npx tailwindcss -i ./client/style.css -o ./public/style.css --watch",
    "serve": "nodemon --watch server server/index.js",
    "dev": "npm-run-all --parallel watch serve",
    "test": "jest"
